{
  "name": "cizzuk-net-builder",
  "version": "0",
  "author": "Cizzuk",
  "description": "Build script for Cizzuk.net Jekyll website",
  "main": "builder.ts",
  "scripts": {
    "build": "BUILD_START_TIME=$(node -p 'Date.now()') bun builder.ts",
    "server": "bun server.ts",
    "dev": "bun run build && bun server.ts --dev",
    "clean": "rm -rf dist _site src/.jekyll-cache"
  },
  "devDependencies": {
    "@types/brotli": "^1.3.4",
    "@types/html-minifier-terser": "^7.0.2",
    "@types/js-yaml": "^4.0.9"
  },
  "dependencies": {
    "brotli": "^1.3.3",
    "html-minifier-terser": "^7.2.0",
    "js-yaml": "^4.1.0",
    "terser": "^5.24.0"
  }
}
