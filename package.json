{
  "name": "cizzuk-net",
  "version": "0.0.0",
  "description": "Build script for Cizzuk.net",
  "main": ".eleventy.js",
  "scripts": {
    "build:css": "sass public/assets/styles.scss:public/assets/styles.css --no-source-map",
    "prebuild": "rm -r _site/ && cp -r public/ _site/",
    "build": "npm run prebuild && npm run build:css && eleventy",
    "debug": "npm run prebuild && npm run build:css && run-p debug:11ty",
    "debug:11ty": "eleventy --serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cizzuk/Cizzuk.net.git"
  },
  "author": "Cizzuk",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/Cizzuk/Cizzuk.net/issues"
  },
  "homepage": "https://github.com/Cizzuk/Cizzuk.net#readme",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "npm-run-all": "^4.1.5",
    "sass": "^1.77.0",
    "luxon": "^3.5.0"
  }
}
